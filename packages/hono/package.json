    "tsup": "^8.0.0",
    "typescript": "^5.0.0",
    "hono": "^4.8.4"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/stukennedy/wsx.git",
    "directory": "packages/hono"
  }
