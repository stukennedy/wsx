  "devDependencies": {
    "tsup": "^8.0.0",
    "typescript": "^5.0.0",
    "@types/node": "^20.0.0",
    "express": "^4.18.0",
    "ws": "^8.0.0",
    "@types/express": "^4.17.0",
    "@types/ws": "^8.0.0"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/stukennedy/wsx.git",
    "directory": "packages/express"
